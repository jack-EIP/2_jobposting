                    </div>
                    <button type="submit" class="btn btn-primary">Submit</button>
                </form> -->
            </div>
        </div>
    </div>
    <!-- slider_area_end -->
    <div class="container-fluid action-content">
        <div style="padding: 10px 0px 10px 0px;" class="container">
            <a href="/employer_dashboard/jobposting" class="btn btn-danger btn-icon-split">
            <span class="text"><%= totalJob %> việc đang đăng</span>
            </a>
        </div>
    </div> 

    <% if (test == 2) { %>  
