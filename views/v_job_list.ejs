    <!-- slider_area_start -->
    <div style="height: 50%; padding-top: 50px; background-image: url(../../page_joblist/img/banner/banner.png);" class="container-fluid">
        <div class="row">
            <div align="center" class="col">
                <img style="height: 80%;" src="../../page_index/img/banner/illustration.png" alt="">
            </div>
            <div style="padding-top: 100px;" class="col catagory_area">
                <h2 style="margin-bottom:10px; color: white;">TÌM VIỆC LÀM MỚI</h2>
                <% if (test == 1) { %>
                    <div align="center" class="alert alert-danger" role="alert">
                        Mời bạn chọn 1 trong 3 thông tin
                    </div>
                <% } %>
                <form action="/joblist/search" method="post">
                    <div class="row cat_search">
                        <div class="col">
                            <div class="single_input">
                                <input name="Chucdanh" type="text" placeholder="Chức danh...">
                            </div>
                        </div>
                        <div class="col">
                            <div class="single_input">
                                <select name="Nganhnghe" class="wide" >
                                    <option value="0" selected>Vui lòng chọn ngành nghề...</option>
                                    <option>Tiếp thị Marketing / Quảng cáo</option>
                                    <option>Tư vấn bán hàng</option>
                                    <option>Y tế</option>
                                    <option>Công nghệ phần mềm</option>
                                    <option>CNTT/PHẦN CỨNG - MẠNG</option>
                                    <option>Luật/Pháp lý</option>
                                    <option>Phiên dịch</option>
                                </select>
                            </div>
                        </div>
                        <div class="col"> 
                            <div class="single_input">
                                <select name="Noilamviec" class="wide">
                                    <option value="0" selected>Vui lòng chọn nơi làm việc...</option>
                                    <option>Thành phố Hồ Chí Minh</option>
                                    <option>Đồng Nai</option>
                                    <option>Đà Nẵng</option>
                                    <option>Hà Nội</option>
                                </select>
                            </div>
                        </div>
                    </div>
                    <div align="center" class="job_btn">
                        <button type="submit" class="boxed-btn3">Tìm việc</button>
                    </div>
                </form>

                <!-- <form>
                    <div class="form-group">
                        <h2 style="color: white;">Email address</h2>
