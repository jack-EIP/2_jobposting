    <div style="margin: 20px 100px 0px 100px" class="job_details_area">
        <div class="container-fluid"> 
            <div class="">
                <div class="">
                    <div class="job_details_header">
                        <div class="single_jobs white-bg d-flex justify-content-between">
                            <div class="jobs_left d-flex align-items-center">
                                <div class="thumb">
                                    <img src="../page_jobdetails/img/svg_icon/1.svg" alt="">
                                </div>
                                <div class="jobs_conetent">
                                    <a href="#"><h4><%= Job_detail.Chucdanh%></h4></a>
                                    <div class="links_locat d-flex align-items-center">
                                        <div class="location">
                                            <p> <i class="fa fa-map-marker"></i> <%= Job_detail.Noilamviec%></p>
                                        </div>
                                        <div class="location">
                                            <p> <i class="fa fa-clock-o"></i> Full-time</p>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div align="right" class="col-4">
                                <a href="" class="boxed-btn3" data-toggle="modal" data-target="#logoutModal">Apply Now</a>
                            </div>
                        </div>
                    </div>
                    <div class="descript_wrap white-bg">
                        <div class="single_wrap">
                            <h4>Job description</h4>
                            <pre style="font-family: Verdana, Geneva, Tahoma, sans-serif;"><%= Job_detail.Mota%></pre>
                        </div>
                        <div class="single_wrap">
                            <h4>Qualifications</h4>
                            <pre style="font-family: Verdana, Geneva, Tahoma, sans-serif;"><%= Job_detail.Yeucaucongviec%></pre>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    
    <!-- Logout Modal-->
    <div class="modal fade" id="logoutModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="false ">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="exampleModalLabel">Xác nhận</h5>
                    <button class="close" type="button" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">×</span>
                    </button>
                </div>
                <div class="modal-body">Bạn có chắc chắn ?</div>
                <div class="modal-footer">
                    <button class="btn btn-secondary" type="button" data-dismiss="modal">Không</button>
                    <form method="post" action="/joblist">
                        <button class="btn btn-secondary" type="submit" value="Submit"/>Có</button>
                    </form>
                </div>
            </div>
        </div>
    </div>

    <!-- footer start -->
